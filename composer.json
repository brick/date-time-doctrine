{
    "name": "brick/date-time-doctrine",
    "description": "Doctrine type mappings for brick/date-time",
    "type": "library",
    "keywords": [
        "Brick",
        "DateTime",
        "Doctrine"
    ],
    "license": "MIT",
    "require": {
        "brick/date-time": "~0.6.0 || ~0.7.0 || ~0.8.0",
        "doctrine/dbal": "^4.0",
        "doctrine/orm": "^3.0",
        "php": "^8.1"
    },
    "require-dev": {
        "ext-pdo": "*",
        "ext-pdo_sqlite": "*",
        "phpunit/phpunit": "^10.5",
        "php-coveralls/php-coveralls": "^2.4",
        "vimeo/psalm": "6.3.0",
        "guzzlehttp/guzzle": "^7.0",
        "symfony/cache": "^5.0 || ^6.0 || ^7.0"
    },
    "autoload": {
        "psr-4": {
            "Brick\\DateTime\\Doctrine\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Brick\\DateTime\\Doctrine\\Tests\\": "tests/"
        }
    }
}
